int f(int(int) modifier, int x) {
  return modifier(x);
}

int main() {
  auto incr = (int x) => int { return x + 1; };
  auto add2 = (int x) => int { return x + 2; };
  auto div2 = (int x) => int { return x / 2; };

  printInt(f(incr, 10));
  printInt(f(add2, -15));
  printInt(f(div2, 43));

  return 0;
}
