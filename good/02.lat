int max(int a, int b) {
  if (a > b) {
    return a;
  }
  return b;
}

int min(int a, int b) {
  if (a < b) {
    return a;
  }
  return b;
}

int main() {
  int a = 2 + 2 * 2;
  int b = 120 / 9 - 8;
  printInt(a % b);
  printInt(max(a, b));
  printInt(min(a, b));
  printBool(a == a);
  printBool(a == b);
  printBool(true && false);

  return 0;
}
